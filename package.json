{
  "name": "paginate-mcp",
  "version": "1.0.0",
  "description": "MCP server for executing shell commands with pagination support",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "paginate-mcp": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "prettier:check": "prettier --check .",
    "prettier": "prettier --write .",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "command",
    "executor",
    "pagination",
    "shell",
    "cli",
    "anthropic",
    "claude"
  ],
  "author": "AndrÃ© Stephano",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/andrelip/paginate-mcp.git"
  },
  "homepage": "https://github.com/andrelip/paginate-mcp#readme",
  "bugs": {
    "url": "https://github.com/andrelip/paginate-mcp/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.57.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.31.0",
    "prettier": "^3.5.3",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  }
}
